{
	"name": "coldtrick/saml_sso",
	"description": "Provides SAML 2.0 based Single Sign On features for Elgg based on the OneLogin SAML library.",
	"homepage": "https://www.coldtrick.com",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "sso"],
	"license": "GPL-2.0-only",
	"support": {
		"source": "https://github.com/ColdTrick/saml_sso",
		"issues": "https://github.com/ColdTrick/saml_sso/issues"
	},
	"require": {
		"composer/installers": "^1.0.8",
		"onelogin/php-saml": "~4.0.0"
	},
	"require-dev": {
		"coldtrick/sniffs": "dev-master"
	},
	"conflict": {
		"elgg/elgg": "<4.3"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
